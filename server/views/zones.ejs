    <script type="text/javascript">
      var $cntlZoneTable = $('#cntl-zone-table');
      var $openZoneTable = $('#open-zone-table');

      function initTables() {
        $cntlZoneTable.bootstrapTable({
            url: "/api/zones/getControl",
            columns: [
              {
                field: 'name',
                title: 'Name',
                sortable: true
              },
              {
                field: 'started',
                title: 'Start Time',
                sortable: true,
                formatter: function (value) {
                  return `<span>${(value == 0 ? "" : new Date(value).toLocaleString())}</span>`;
                }
              },
              {
                field: 'actions',
                title: 'Actions',
                sortable: false,
                events: operateEvents,
                formatter: cntlOperateFormatter
              }

            ]
        });

        $openZoneTable.bootstrapTable({
            url: "/api/zones/get",
            columns: [
              {
                field: 'name',
                title: 'Name',
                sortable: true
              },
              {
                field: 'area',
                title: 'Area (sqft)',
                sortable: true
              },
              {
                field: 'swhc',
                title: 'Soil',
                sortable: true,
                formatter: function (value) {
                  var soilWHC = [ 0.75, 1.25, 1.5, 2.0 ];
                  var soilTypes = [ "Coarse", "Sandy", "Medium", "Fine" ];
                  for ( var i = 0; i < soilWHC.length; i++ )
                    if (value === soilWHC[i]) return `<span>${soilTypes[i]}</span>`;
                }
              },
              {
                field: 'irreff',
                title: 'Irrigation',
                sortable: true,
                formatter: function (value) {
                  var irrEff = [ 0.9, 0.8 ];
                  var irrTypes = [ "Drip", "Spray" ];
                  for ( var i = 0; i < irrEff.length; i++ )
                    if (value === irrEff[i]) return `<span>${irrTypes[i]}</span>`;
                }
              },
              {
                field: 'flowrate',
                title: 'Flow Rate (gpm)',
                sortable: true,
                formatter: function (value) {
                  var rates = [ 0.5, 1, 2 ];
                  var flowTypes = [ "0.5", "1.0", "2.0" ];
                  for ( var i = 0; i < rates.length; i++ )
                    if (value === rates[i]) return `<span>${flowTypes[i]}</span>`;
                }
              },
              {
                field: 'started',
                title: 'Start Time',
                sortable: true,
                formatter: function (value) {
                  return `<span>${(value == 0 ? "" : new Date(value).toLocaleString())}</span>`;
                }
              },
              {
                field: 'actions',
                title: 'Actions',
                sortable: false,
                events: operateEvents,
                formatter: openOperateFormatter
              }
            ]
        });
      }

